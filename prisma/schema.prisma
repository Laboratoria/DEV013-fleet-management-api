generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Taxis {
  id Int @id @default(autoincrement())         
  plate String @db.VarChar(200)
}

model Trajectories {
  id Int @id
  taxi_id Int 
  date DateTime
  latitude Float // Campo de tipo Double
  longitude Float
}

